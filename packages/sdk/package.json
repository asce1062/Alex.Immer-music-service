{
  "name": "@asce1062/music-service-sdk",
  "version": "1.0.0-beta.11",
  "description": "Official TypeScript/JavaScript SDK for Alex.Immer Music Service API",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "build:watch": "tsup --watch",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "format": "prettier --write \"src/**/*.{ts,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,json}\"",
    "prepublishOnly": "npm run build && npm run test && npm run typecheck"
  },
  "keywords": [
    "music",
    "audio",
    "streaming",
    "sdk",
    "typescript",
    "cloudfront",
    "player",
    "download"
  ],
  "author": "Alex Mbugua <tnkratos@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/asce1062/Alex.Immer-music-service/tree/main/packages/sdk",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/asce1062/Alex.Immer-music-service.git",
    "directory": "packages/sdk"
  },
  "bugs": {
    "url": "https://github.com/asce1062/Alex.Immer-music-service/issues"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "flexsearch": "^0.7.43",
    "jsmediatags": "^3.9.7",
    "workbox-cacheable-response": "^7.0.0",
    "workbox-core": "^7.0.0",
    "workbox-expiration": "^7.0.0",
    "workbox-range-requests": "^7.0.0",
    "workbox-routing": "^7.0.0",
    "workbox-strategies": "^7.0.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.10.6",
    "@typescript-eslint/eslint-plugin": "^6.16.0",
    "@typescript-eslint/parser": "^6.16.0",
    "@vitest/coverage-v8": "^1.1.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.2",
    "jsdom": "^24.1.3",
    "msw": "^2.0.11",
    "prettier": "^3.1.1",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vitest": "^1.1.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
